{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production npm run build-client:production && forever ./bin/www",
    "start-server:development": "cross-env NODE_ENV=development nodemon --ignore 'client/' ./bin/www",
    "start-client:development": "cross-env NODE_ENV=development node ./client/devServer",
    "build-client:production": "cross-env NODE_ENV=production webpack -p --progress",
    "build-client:development": "cross-env NODE_ENV=development webpack --config webpack.config.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arpitgo4/Express-React-Scaffholding"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Arpit Goyal <arpit.go4@gmail.com> (http://github.com/arpitgo4)",
